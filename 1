import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
 cs=pd.read_csv('')
fuel=cs('fuel type')
prices=cs('prices')
plt.bar(fuel,prices($),color="sky blue")
plt.xlabel("fuel")
plt.ylabel("prices($)")
plt.title("")
plt.xticks(rotation=45,ha="right")
plt.tight_layout()
plt.show


import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
cr=pd.csv_read()
x=cr('age')
y=cr('price')
fig,ax=plt.subplots()
plt.scatter(x,y,color="red",marker="^",s=20)
plt.xlabel("age")
plt.ylabel("price")
plt.title("")
