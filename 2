import matplotlib.pyplot as plt
sub=['physics','Maths','chemistry','dadc','python']
marks=[23,45,67,98,43]
plt.plot(sub,marks)
plt.xlabel("sub")
plt.ylabel("marks")
plt.show()


import matplotlib.pyplot as plt
sub=['physics','Maths','chemistry','dadc','python']
marks=[23,45,67,98,43]
plt.plot(sub,marks,marker='o',linestyle='-',linewidth=2,color='blue')
plt.xlabel("sub")
plt.ylabel("marks")
plt.show()
